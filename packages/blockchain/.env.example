# Server Configuration
PORT=3002
NODE_ENV=development
CORS_ORIGINS=http://localhost:3000,https://scamdunk.dev

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Cache TTL (seconds)
REDIS_TTL_TOKEN_DATA=300
REDIS_TTL_CONTRACT_ANALYSIS=1800
REDIS_TTL_PRICE_DATA=60
REDIS_TTL_WALLET_REPUTATION=3600

# Blockchain RPC URLs
ETHEREUM_RPC_URL=https://eth-mainnet.g.alchemy.com/v2/your-api-key
BSC_RPC_URL=https://bsc-dataseed1.binance.org/
POLYGON_RPC_URL=https://polygon-rpc.com/
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
BITCOIN_RPC_URL=https://blockstream.info/api

# Blockchain Explorer API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key
BSCSCAN_API_KEY=your_bscscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# External API Keys
COINGECKO_API_KEY=your_coingecko_pro_api_key
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key
DEFIPULSE_API_KEY=your_defipulse_api_key

# Security
JWT_SECRET=your-super-secret-jwt-key-change-in-production
API_KEYS=api_key_1,api_key_2,api_key_3

# Logging
LOG_LEVEL=info